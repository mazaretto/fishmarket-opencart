{{ header }}
<div id="error-not-found" class="error-not-found">

  <header class="pages-header">
    <div class="container">
      <h1 class="pages-header__title">{{ heading_title }} {% if weight %} ({{ weight }}) {% endif %}</h1>
      <p>{{ text_error }}</p>
      <ul class="pages-header__breadcrumbs">
        {% for breadcrumb in breadcrumbs %}
          <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </header>

  <div id="content" class="container error-not-found__cotnainer">

  </div>
</div>
{{ footer }}
